              <%= render partial: 'games/round', locals: {round: CURRENT_ROUND.to_i-1, games: @round_games} if @round_games and @round_games.any? %>
            </div>
            <div class="col-xs-12 col-sm-12 col-md-12" id="table_home">
              <%= render partial: 'teams/table', locals: {teams: @teams} if @teams and @teams.any? %>
            </div>
          </div>
        </div>
      </div>
    </div>
    <footer id="footer">
<div class="container">
  <div class="row content">    
