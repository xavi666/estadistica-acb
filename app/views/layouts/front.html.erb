    <%= csrf_meta_tags %>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <link href="https://fonts.googleapis.com/css?family=Teko" rel="stylesheet">
    <%= display_meta_tags %>
    <%= render partial: "front/google_analytics" %>
